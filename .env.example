# Application
NODE_ENV=development
PORT=3000

# Database Configuration (PostgreSQL)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=travel_planning
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# Azure OpenAI
AZURE_OPENAI_ENDPOINT=https://your-openai-instance.openai.azure.com/
AZURE_OPENAI_KEY=your-openai-key

# Azure Cosmos DB
COSMOS_DB_ENDPOINT=https://your-cosmos-instance.documents.azure.com:443/
COSMOS_DB_KEY=your-cosmos-key
COSMOS_DB_DATABASE_ID=travel-planning
COSMOS_DB_CONTAINER_ID=trip-itineraries
COSMOS_CONNECTION_STRING=your-cosmos-connection-string

# Azure Service Bus
SERVICEBUS_CONNECTION_STRING=Endpoint=sb://your-servicebus.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key

# Azure Event Hubs
EVENTHUB_CONNECTION_STRING=Endpoint=sb://your-eventhub.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key

# Azure Web PubSub
WEBPUBSUB_CONNECTION_STRING=Endpoint=https://your-webpubsub.webpubsub.azure.com;AccessKey=your-key;Version=1.0

# Azure Cognitive Search
AZURE_SEARCH_ENDPOINT=https://your-search-service.search.windows.net
AZURE_SEARCH_KEY=your-search-key

# Authentication
GOOGLE_CLIENT_ID=your-google-client-id