# Application
NODE_ENV=development
PORT=3000

# Database Configuration (PostgreSQL)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=travel_planning
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password

# OpenAI API
OPENAI_API_KEY=your-openai-api-key

# Azure Cosmos DB
COSMOS_DB_ENDPOINT=https://your-cosmos-instance.documents.azure.com:443/
COSMOS_DB_KEY=your-cosmos-key
COSMOS_DB_DATABASE_ID=travel-planning
COSMOS_DB_CONTAINER_ID=trip-itineraries
COSMOS_CONNECTION_STRING=your-cosmos-connection-string

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379

# Redis replaced Azure Service Bus, Event Hubs, Web PubSub, and Cognitive Search
# No additional configuration needed - PostgreSQL + pgvector handles search

# Authentication
GOOGLE_CLIENT_ID=your-google-client-id